{
    "theme": "Tomorrow",
    "themeDark": "Tomorrow Night",
    "defaultProfile": "fish",
    "profiles": {
        "fish": {
            "command": "/opt/homebrew/bin/fish"
        },
        "sunbeam": {
            "command": "/opt/homebrew/bin/fish",
            "args": [
                "-c",
                "sunbeam"
            ]
        },
        "zsh": {
            "command": "/bin/zsh",
            "args": [
                "-l"
            ]
        }
    }
}
